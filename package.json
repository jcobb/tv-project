{
  "name": "Base",
  "version": "1.0.0",
  "description": "Basic structure for Express web apps.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test": "jest",
    "test-react": "jest"
  },
  "keywords": [
    "base",
    "project",
    "node",
    "express",
    "structure"
  ],
  "author": "Terlici Ltd.",
  "license": "MIT",
  "dependencies": {
    "express": "^4.9.5",
    "jade": "^1.7.0",
    "restler": "^3.2.2",
    "promise": "^6.1.0",
    "xml2js": "^0.4.5",
    "jspm": "^0.14.0",
    "gulp": "^3.8.11",
    "gulp-react": "^3.0.0",
    "gulp-watch": "^4.1.1",
    "gulp-plumber": "^1.0.0",
    "gulp-sass": "^1.3.3",
    "react-tools": "^0.12.2",
    "react": "^0.12.2"
  },
  "devDependencies": {
    "mocha": "^1.21.4",
    "should": "^4.0.4",
    "jest-cli": "^0.4.0",
    "babel-jest": "^4.0.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "public",
      "lib": "public",
      "packages": "public/vendor"
    },
    "configFile": "public/vendor/config.js",
    "dependencies": {
      "express": "^4.9.5",
      "jade": "^1.7.0",
      "jspm": "^0.14.0",
      "promise": "^6.1.0",
      "react": "npm:react@^0.12.2",
      "reqwest": "github:ded/reqwest@^1.1.5",
      "restler": "^3.2.2",
      "xml2js": "^0.4.5"
    }
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js"],
    "moduleFileExtensions": ["js", "json", "es6"],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathDirs": [
      "<rootDir>/__tests__"
    ]
  }
}
